{
  "name": "backend",
  "version": "0.0.1",
  "description": "Express server",
  "main": "./dist/index.js",
  "engines": {
    "node": "v14.17.4"
  },
  "scripts": {
    "start": "npm run build && node ./dist/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --pretty --transpile-only ./src/index.ts",
    "t": "ts-node -r tsconfig-paths/register ./src/test.ts",
    "db:seed": "npm run db:seed:admin && npm run db:seed:content",
    "db:seed:admin": "ts-node -r tsconfig-paths/register ./src/utils/seeds/createAdmin.ts",
    "db:seed:content": "ts-node -r tsconfig-paths/register ./src/utils/seeds/populate.ts",
    "prettier": "npx prettier --write ./src/",
    "prettier:check": "npx prettier --check ./src/",
    "build": "tsc --project ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express"
  ],
  "author": "Juan Wagner",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.6.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "mongoose": "^5.12.14",
    "multer": "^1.4.2",
    "nanoid": "^3.1.23",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.3",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "socket.io": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/node": "^15.0.3",
    "@types/node-fetch": "^2.5.12",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "prettier": "2.3.2",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
